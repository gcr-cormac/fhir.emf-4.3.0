                <xs:documentation xml:lang="en">Condition for which the medicinal use applies. (choose any one of indication*, but only one)</xs:documentation>
              </xs:annotation>
             </xs:element>
            <xs:element minOccurs="0" name="indicationReference" type="Reference">
              <xs:annotation>
                <xs:documentation xml:lang="en">Condition for which the medicinal use applies. (choose any one of indication*, but only one)</xs:documentation>
              </xs:annotation>
             </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="status" type="CodeableConcept">
